{
  "expo": {
    "name": "Cat's collection",  // changed name 
    "slug": "cats-collection",   // changed slug
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/cat_images/collection.jpg",  // default splash before start all rout 
      "resizeMode": "contain",
      "backgroundColor": "#222"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "package": "com.anonymous.myapp",
      "runtimeVersion": "1.0.0"
    },
    "web": {
     
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"   // add my Cat's favicon
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "76e4de11-4ae9-4325-9292-478e05d26600"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/76e4de11-4ae9-4325-9292-478e05d26600"
    }
  }
}
